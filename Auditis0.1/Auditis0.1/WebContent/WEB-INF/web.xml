<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Auditis0.1</display-name>
<servlet>
		<servlet-name>Auditis</servlet-name>
		<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
		<!-- Dispatcher servlet is the frontcontroller here which transfer the 
			control to the mappping file Auditis-servlet.xml -->

		<!-- 3.The DispatcherServlet will take help from ViewResolver to pickup 
			the defined view for the request -->
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>Auditis</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
</web-app>
